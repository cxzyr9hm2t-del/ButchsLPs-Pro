      <script>
                document.addEventListener('DOMContentLoaded', function() {
                              // Add interactive click feedback
                              const links = document.querySelectorAll('.deployment-link, .feature-link, .action-btn');
                              links.forEach(link => {
                                                link.addEventListener('click', function(e) {
                                                                      this.style.transform = 'scale(0.95)';
                                                                      setTimeout(() => this.style.transform = '', 100);
                                                });
                              });

                              // Animate status indicators  
                              document.querySelectorAll('.status.ready').forEach(status => {
                                                status.style.animation = 'pulse 2s infinite';
                              });

                              // Add pulse animation
                              const style = document.createElement('style');
                              style.textContent = '@keyframes pulse { 0% { opacity: 1; } 50% { opacity: 0.7; } 100% { opacity: 1; } }';
                              document.head.appendChild(style);
                });

                // Keyboard shortcuts
                document.addEventListener('keydown', function(e) {
                              if (e.ctrlKey || e.metaKey) {
                                                if (e.key === 'g') {
                                                                      e.preventDefault();
                                                                      window.open('https://github.com/cxzyr9hm2t-del/ButchsLPs-Pro', '_blank');
                                                } else if (e.key === 'd') {
                                                                      e.preventDefault();  
                                                                      window.open('https://butchslps-pro.vercel.app', '_blank');
                                                }
                              }
                });
      </script>
  </body>
